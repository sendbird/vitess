{
  "sharded": true,
  "vindexes": {
    "hash_index": {
      "type": "hash"
    },
    "lookup_vdx": {
      "type": "lookup_hash_unique",
      "params": {
        "table": "lookup",
        "from": "name",
        "to": "id",
        "autocommit": "true"
      },
      "owner": "user"
    }
  },
  "tables": {
    "user": {
      "column_vindexes": [
        {
          "column": "user_id",
          "name": "hash_index"
        },
        {
          "column": "name",
          "name": "lookup_vdx"
        }
      ]
    },
    "lookup": {
      "column_vindexes": [
        {
          "column": "id",
          "name": "hash_index"
        }
      ]
    }
  }
}
	